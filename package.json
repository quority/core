{
  "name": "mw.js",
  "version": "2.4.1",
  "description": "Typed MediaWiki API client for node.js using TypeScript.",
  "main": "dist/main.js",
  "files": [
    "dist/**",
    "!dist/demo/**",
    "!dist/tests/**"
  ],
  "types": "dist/main.d.ts",
  "scripts": {
    "build": "yarn tsc --listEmittedFiles --incremental",
    "re-build": "rimraf tsconfig.tsbuildinfo dist && yarn tsc --listEmittedFiles --incremental",
    "lint": "eslint src/ --ext .ts --cache",
    "lint:fix": "eslint src/ --fix --ext .ts --cache",
    "hooks": "husky install .github/husky",
    "test": "mocha ./dist/tests"
  },
  "keywords": [
    "Fandom",
    "MediaWiki",
    "MediaWiki API",
    "wiki"
  ],
  "author": "bitomic",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/bitomic/mwjs.git"
  },
  "homepage": "https://github.com/bitomic/mwjs#readme",
  "dependencies": {
    "fs-extra": "^10.1.0",
    "tmp-promise": "^3.0.3",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@types/form-data": "2.5.0",
    "@types/fs-extra": "9.0.13",
    "@types/mocha": "9.1.1",
    "@types/node": "16.11.41",
    "@types/tough-cookie": "4.0.2",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "dotenv": "16.0.1",
    "eslint": "8.18.0",
    "husky": "8.0.1",
    "lint-staged": "^13.0.3",
    "mocha": "10.0.0",
    "rimraf": "3.0.2",
    "typescript": "4.7.4"
  },
  "packageManager": "yarn@3.2.1"
}
